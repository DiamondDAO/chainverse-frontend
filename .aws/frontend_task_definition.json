{
  "containerDefinitions": [
    {
      "name": "frontend",
      "image": "823557601923.dkr.ecr.us-east-2.amazonaws.com/chainverse/frontend:latest",
      "memory": 500,
      "cpu": 200,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 0,
          "protocol": "tcp"
